{
  "name": "rural24-backend-api",
  "version": "2.0.0",
  "description": "Rural24 REST API - Fastify Backend",
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:watch": "tsx watch --clear-screen=false src/server.ts",
    "dev:test": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "health": "node -e \"fetch('http://localhost:3000/api/health').then(r=>r.json()).then(console.log).catch(e=>process.exit(1))\""
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@supabase/supabase-js": "^2.89.0",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "sharp": "^0.34.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
